{
  "name": "canarist",
  "version": "1.0.1",
  "main": "index.js",
  "bin": "index.js",
  "author": "Bj√∂rn Brauer <bjoern.brauer@xing.com>",
  "license": "MIT",
  "scripts": {
    "fmt": "prettier --write '*.js' 'README.md'",
    "test": "echo 'no tests' && exit 0"
  },
  "dependencies": {
    "cosmiconfig": "^5.2.1",
    "debug": "^4.1.1",
    "fast-glob": "^3.0.0",
    "git-url-parse": "^11.1.2",
    "make-dir": "^3.0.0",
    "merge-options": "^1.0.1",
    "minimist": "^1.2.2",
    "semver": "^6.1.1",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":preserveSemverRanges"
    ],
    "lockFileMaintenance": {
      "enabled": true
    }
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "singleQuote": true,
    "proseWrap": "never"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "README.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
